{
    "network": "DeepFakeClassifierWithSimpleEnD",
    "encoder": "tf_efficientnet_b7_ns",
    "batches_per_epoch": 2500,
    "size": 380,
    "fp16": true,
    "optimizer": {
        "batch_size": 7,
        "type": "SGD",
        "momentum": 0.9,
        "weight_decay": 1e-4,
        "learning_rate": 0.01,
        "nesterov": true,
        "schedule": {
            "type": "poly",
            "mode": "step",
            "epochs": 40,
            "kill_early": 5,
            "params": {"max_iter":  100500}

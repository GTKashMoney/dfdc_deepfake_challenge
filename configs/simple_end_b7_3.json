    },
    "normalize": {
        "mean": [0.485, 0.456, 0.406],
        "std": [0.229, 0.224, 0.225]
    },
    "losses": {
        "BinaryCrossentropy": 1
    },
    "end": {
        "alpha": 0.3,
        "beta": 0.65,
        "end_weight": 4
    },
    "load_pretrained": {
        "load": true,
        "path": "weights/final_999_DeepFakeClassifier_tf_efficientnet_b7_ns_0_23"
    }
}

